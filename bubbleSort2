
#include <iostream>
#include <cmath>
#include <vector>
#include <string>

using namespace std;

void bubbleSort(int arr[], int n) {
    
    //two index variables
    int i, j;
    
    //goes through the entire array
    for (i = 0; i < (n-1); i++) {
        
        //initializes swapped to be false (becomes true if later term is greater than previous)
        swapped = false;
        for (j = 0; j < (n-i-1); j++){
            
            if (arr[j] > arr[j+1]){
                swap(&arr[j], &arr[j+1}]);
                swapped = true;
            }
        }
        
        //catches case if array is already sorted
        if (swapped == false) {
            break;
        }
    }
    
    
    
}

void swap(int *xp, int *yp){
    
    int temp = *xp;
    *xp = *yp;
    *yp = temp;
}

void printArray(int array[], int length) {
    
    int i;
    for (i = 0; i < length; i++)
    {
        cout << array[i] << " " << endl;
    }
}



int main() {
    
    
    
    
}
